{
    "extends": "standard",
    "rules": {
      "no-extra-boolean-cast": 0,
      "no-console": 0,
      "strict": 0,
      "eqeqeq": 0,
      "no-underscore-dangle": 0,
      "space-infix-ops": 0,
      "no-use-before-define": 0,
      "quotes": [0, "single"],
      "no-prototype-builtins": 0,
      "no-mixed-spaces-and-tabs": 0,
      "no-unused-vars": 0,
      "no-useless-escape": 0,
      "no-useless-catch": 0,
      "vue/require-prop-type-constructor": 0,
      "vue/no-use-v-if-with-v-for": 0
    }
}